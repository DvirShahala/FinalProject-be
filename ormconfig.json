{

   "url": "${process.env.DATABASE_URL}",
    "type": "postgres",
   "entities": [
      "src/entity/**/*.ts"
   ],
   "synchronize": true,
   "migrations": [
      "src/migration/**/*.ts"
   ],
   "subscribers": [
      "src/subscriber/**/*.ts"
   ],
   "cli": {
      "entitiesDir": "src/entity",
      "migrationsDir": "src/migration",
      "subscribersDir": "src/subscriber"
   }
}